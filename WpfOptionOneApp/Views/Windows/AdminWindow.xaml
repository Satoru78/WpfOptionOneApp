        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfOptionOneApp.Views.Windows"
        mc:Ignorable="d"
        Title="Вы авторизовались как администратор" Height="450" Width="800">
    <Grid>

        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Top">
            <Button Content="Назад" Name="AdminWindowBack" Click="AdminWindowBack_Click" Width="150" Margin="5,15,0,0" ></Button> 
